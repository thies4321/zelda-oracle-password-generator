{
  "name": "thies4321/zelda-oracle-password-generator",
  "description": "Password generator for The Legend of Zelda Oracle of Seasons/Ages",
  "license": "MIT",
  "type": "standard",
  "homepage": "https://github.com/thies4321/zelda-oracle-password-generator",
  "require": {
    "php": "^8.2.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.37",
    "psy/psysh": "^0.11.21",
    "phpunit/phpunit": "^10",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "Thies\\ZeldaOraclePasswordGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Thies\\ZeldaOraclePasswordGenerator\\Tests\\": "tests/"
    }
  },
  "bin": [
    "bin/zelda-oracle-password-generator"
  ],
  "archive": {
    "exclude": [
      "docker-compose.yml",
      "Dockerfile"
    ]
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  }
}
